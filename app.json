{
  "expo": {
    "name": "TrustMatch",
    "slug": "trustmatch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./client/public/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./client/public/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trustmatch.app",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./client/public/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.trustmatch.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./client/public/logo.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}